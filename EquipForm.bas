Private Sub Form_Load()
'Hides archive button and recurrance tabs from non-admins
If DLookup("[Contacts].[Admin]", "Contacts Extended", "MyID ='" & UserNameWindows & "'") = False Then
Me.Command31.Visible = False
Me.Recurrance.Visible = False
Else

End If
End Sub
Private Sub Command31_Click()
On Error GoTo Err_Handler
'Write a new record to the sumbissions table
Set Db = CurrentDb
Set rs = Db.OpenRecordset("MERMArchive")
Select Case MsgBox("Are you sure you want to archive this item?" & vbNewLine & _
"IMPORTANT: This action cannot be undone." & vbCrLf & vbLf & "  Yes:         Archive the item" _
& vbCrLf & "  No:          Do NOT Archive" & vbCrLf, vbYesNo + vbQuestion, "Archive Current Record?")
'& vbCrLf & "  Cancel:    Reset (Undo) Changes" & vbCrLf, vbYesNoCancel + vbQuestion, "Save Current Record?")
        
        Case vbYes: 'Archive the item
    Dim strComment As String
        strComment = InputBox(Prompt:="Please provide any comments or other justification for archiving this item.", _
             Title:="Archive Comments", Default:="Enter Your Comments")
             

'Checks if inputbox string is empty and if it's not it adds these fields to the table
If strComment <> "" Then
        rs.AddNew
            rs("EventID").Value = Me.EventID
            rs("Site").Value = Me.Site
            rs("CommonName").Value = Me.CommonName
            rs("DocumentName").Value = Me.DocumentName
            rs("Attachment").Value = Me.Attachment
            rs("AQMD_ID").Value = Me.AQMD_ID
            rs("Asset").Value = Me.Asset
            rs("Email").Value = Me.Email
            rs("Comments").Value = strComment
        rs.Update

'If input box is empty then it exits the sub
ElseIf strComment = "" Or strComment = vbNullString Then 'They canceled (VbCancel)
       Exit Sub
End If
DoCmd.RunSQL "DELETE FROM [MERM] Where [EventID] = EventID.Value;"

DoCmd.Close acForm, "EquipForm", acSaveNo

        Case vbNo: 'Do not save or undo
            'Do nothing

        'Case vbCancel: 'Undo the changes
            'Do nothing
            'DoCmd.RunCommand acCmdUndo

        Case Else: 'Default case to trap any errors
            'Do nothing
    End Select

Exit_Handler:
    Exit Sub
    
Err_Handler:
    Call LogError(Err.Number, Err.Description, conMod & ".txtCancelRestore_Click")
    Resume Exit_Handler

End Sub

